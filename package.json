{
  "name": "daq-software",
  "version": "1.0.0",
  "description": "Software for a data aquisition unit",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"DAQ Software\"",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChrisNewton51/daq-software.git"
  },
  "keywords": [
    "daq",
    "data",
    "electron",
    "current",
    "voltage",
    "capacitance",
    "tesla",
    "capacitance",
    "inductance"
  ],
  "author": "Chris Newton",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ChrisNewton51/daq-software/issues"
  },
  "homepage": "https://github.com/ChrisNewton51/daq-software#readme",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "electron": "^15.2.0"
  },
  "dependencies": {
    "chart.js": "^3.7.1",
    "csv-parser": "^3.0.0",
    "csv-write-stream": "^2.0.0",
    "csv-writer": "^1.6.0",
    "electron-squirrel-startup": "^1.0.0",
    "fs": "0.0.1-security",
    "jquery": "^3.6.0",
    "remote": "^0.2.6",
    "serialport": "^10.4.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "daq_software"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
